/* storeList/storeList.wxss */
.storeList{
  width: 694rpx;
  height: 170rpx;
  border-bottom: 1px solid #d7d7d7;
  margin: 0 auto;
  display: flex;
  justify-content: space-between
}
.img_box{
  width: 170rpx;
  height: 100%;
  overflow: hidden;
  position: relative
}
.img_box image{
  width: 100%;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  margin-top: auto;margin-bottom: auto
}
.content_box{
  width: 524rpx;
  height: 100%;
   padding-left: 20rpx
}
.first{
  margin-top: 34rpx;
  display: flex;
  justify-content: space-between;
 
}
.first text:nth-child(1){
  width: 220rpx;
  white-space: nowrap;
  color: #303032;
  font-size: 26rpx;
  line-height: 24rpx;
  display: block
}
.first text:nth-child(3){
  color: #A1A1A3;
  font-size: 20rpx;
  line-height: 24rpx
}
.first image{
  width: 24rpx;
  height: 24rpx;
  margin-right: -170rpx
}
.adress{
  color: #6F6F71;
  font-size: 22rpx;
  margin-top: 38rpx;
  line-height: 30rpx
}