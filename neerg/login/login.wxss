/* login/login.wxss */
.call{
  width: 650rpx;
  height: 80rpx;
  margin:30rpx auto;
  border-bottom: 1px solid #e9e9e9;
  position: relative
}
.call input{
  width: 100%;
  height: 100%;
  font-size: 25rpx;
}
.cal{
  font-size: 25rpx;
  color: #636363
}
.sub{
  height: 66rpx;
  position: absolute;
  right: 20rpx;
  top: 0;
  bottom: 0;
  margin-top: auto;
  margin-bottom: auto;
  font-size: 26rpx;
  color: #6b6b6b;
  z-index: 100
}
.btn_box{
  width: 660rpx;
  height: 100rpx;
  background: #46cc8c;
  border-radius: 60rpx;
  font-size: 31rpx;
  color: #fff;
  text-align: center;
  line-height: 100rpx;
  margin: 70rpx auto
}