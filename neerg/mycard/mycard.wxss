/* mycard/mycard.wxss */
.content{
  width: 750rpx;
  height: auto;
}
.list_one{
  width: calc(100vw - 54rpx);
  height: 353rpx;
  border-radius: 20rpx;
  position: relative;
  overflow: hidden;
  box-shadow:0px 6px 22px #d7d7d7;
  margin: 60rpx auto
}
.list_one:nth-child(1){

  margin-top: 37rpx;
}
.list_one image{
  position: absolutel;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%
}
.list_one text:nth-child(2){
  display: block;
  position: absolute;
  bottom: 46rpx;
  right: 30rpx;
  font-size: 20rpx;
  font-weight: 700
}
.list_one text:nth-child(3){
  display: block;
  position: absolute;
  bottom: 19rpx;
  right: 30rpx;
  font-size: 20rpx;
  font-weight: 700
}
.list_one text:nth-child(4){
  display: block;
  position: absolute;
  top: 32rpx;
  right: 40rpx;
  font-size: 31rpx;
  font-weight: bold;
  color: #fff
}
/* 我的G享卡 */
.input_box{
  width: 100%;
  height: 64rpx;
  margin-top: 51rpx;
}
.input{
  float: left;
  border-bottom: 1px solid #888888;
  padding-left: 14rpx;
  font-size: 20rpx;
  margin-left: 20rpx;
  width: 354rpx
}
.get_button{
  float: right
}
.place{
  font-size: 20rpx;
}
.get_button{
  width: 166rpx;
  height: 64rpx;
  background: #46cc8c;
  border-radius: 40rpx;
  font-size: 24rpx;
  font-weight: bold;
  text-align: center;
  line-height: 64rpx;
  color: #fff;
  margin-right: 20rpx
}
.myCard{
  font-size: 24rpx;
  color: #000;
  margin-top: 85rpx;
  font-weight: bold;
  margin-left: 30rpx;
  transition:  all 0.5s
}

.cardName{
  font-size: 40rpx;
  color: #333333;
  text-align: center;
  font-weight: Regular
}
.cardDesc{
  font-size: 22rpx;
  color: #333333;
  margin-top: 36rpx;
  text-align: center;
  display: block;
}
.carddes{
  width: 670rpx;
  margin: 30rpx auto;
  display: flex;
  justify-content: space-between;
  color: #333333;
  font-size: 26rpx;
}
page{
  background: #F8F8F8;
}
.listE{
  width: 750rpx;
  height: auto;
  padding-top: 50rpx;
  margin-top:20rpx
}
.listE:nth-child(1){
  margin-top: 0
}
.zanwushuju{
  width: 400rpx;
  height: 580rpx;
  position: fixed;
  left: 0;
