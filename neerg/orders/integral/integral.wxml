<view style='width:750rpx;height:20rpx;background:#f5f5f5'></view>
<view style='padding-bottom:20rpx'>
  <view wx:for="{{datalist}}" wx:key="" wx:for-index="index">
    <view class='list_one'>
      <view class='cire'></view>
      <view class='add_jf'>+{{item.num}}</view>
      <view class='add_desc'>
        <text>{{item.memo}}</text>
      <text>当前积分{{item.jifen}}</text>
      </view>
    </view> 
    <view class='shu_box' id='{{datalist.length}}' wx:if="{{datalist.length-1!=index}}"></view>
  </view>
</view>