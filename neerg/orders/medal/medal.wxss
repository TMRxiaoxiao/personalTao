/* orders/medal/medal.wxss */
.content{
  width: calc(100vw - 54rpx);
  height: auto;
  margin: 70rpx auto;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between
}
.list_model{
  width: 30%;
  height: 343rpx;
  margin-top: 40rpx;
  position: relative
}
.list_model:nth-child(1),.list_model:nth-child(2),.list_model:nth-child(3){
  margin-top: 0
}
.model_logo{
  width: 199rpx;
  height: 172rpx;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  margin-left: auto;
  margin-right: auto
}
.model_logo image{
  width: 100%;
  height: 100%
}
.get{
  width: 100%;
  color: #808080;
  font-size: 18rpx;
  position: absolute;
  top: 199rpx;
  left: 0;
  text-align: center
}
.dengji{
  width: 100%;
  color: #808080;
  font-size: 20rpx;
  position: absolute;
  top: 230rpx;
  left: 0;
  text-align: center;
  line-height: 36rpx
}