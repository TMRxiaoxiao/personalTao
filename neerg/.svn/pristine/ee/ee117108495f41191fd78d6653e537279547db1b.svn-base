/* 头部导航 */

.tab-h {
  height: 100rpx;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 100rpx;
  font-size: 26rpx;
  white-space: nowrap;
  padding-right: 27rpx;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 10000;
  background: #fff;
  box-shadow:0px 6px 22px rgba(0,0,0,0.1);
}

.tianchong {
  width: 750rpx;
  height: 80rpx;
}

.tab-item:nth-child(1) {
  margin-left: 27rpx;
}

.tab-item {
  margin-left: 68rpx;
  display: inline-block;
  color: #333;
}

.tab-item.active {
  position: relative;
}

.tab-item.active:after {
  content: "";
  display: block;
  height: 4rpx;
  width: 100%;
  background: #5bb580;
  position: absolute;
  bottom: 18rpx;
  left: 0;
  right: 0;
  margin-left: auto;
  margin-right: auto;
  border-radius: 16rpx;
}

.item-ans {
  width: 100%;
  display: flex;
  flex-grow: row no-wrap;
  justify-content: space-between;
  padding: 30rpx;
  box-sizing: border-box;
  height: 180rpx;
  align-items: center;
  border-bottom: 1px solid #f2f2f2;
}

.avatar {
  width: 100rpx;
  height: 100rpx;
  position: relative;
  padding-right: 30rpx;
}

.avatar .img {
  width: 100%;
  height: 100%;
}

.avatar .doyen {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  bottom: -2px;
  right: 20rpx;
}

.expertInfo {
  font-size: 12px;
  flex-grow: 2;
  color: #b0b0b0;
  line-height: 1.5em;
}

.expertInfo .name {
  font-size: 16px;
  color: #000;
  margin-bottom: 6px;
}

.askBtn {
  width: 120rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 14px;
  border-radius: 60rpx;
  border: 1px solid #4675f9;
  color: #4675f9;
}

.tab-content {
  margin-top: 80rpx;
}

.scoll-h {
  height: 100%;
}

.add_box {
  width: 100%;
  height: 90rpx;
  /* background: red; */
}

