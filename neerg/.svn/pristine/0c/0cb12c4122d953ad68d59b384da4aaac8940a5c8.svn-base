  float: right
} 
.content text{
  display: block;
}
.left_box text:nth-child(1){
  font-size: 20rpx;
  color: #fff;
  margin-left: 30rpx;
  margin-top: 26rpx
}
.left_box text:nth-child(2){
  font-size: 16rpx;
  color: #fff;
  margin-left: 30rpx;
  margin-top: 10rpx
}
.left_box text:nth-child(3){
  display: inline-block;
  font-size: 20rpx;
  color: #fff;
  margin-left: 30rpx;
  margin-top: 22rpx;
  border-bottom: 1px solid #fff
}
.right_box text:nth-child(1){
  font-size: 24rpx;
  color: #fff;
  display: block;
  margin-left: 9rpx;
  margin-top: 14rpx;
  white-space: nowrap
}
.right_box text:nth-child(2){
  font-size: 18rpx;
  color: #fff;
  display: block;
  margin-left: 20rpx;
  margin-top: 40rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.content image{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1
}


