  background: red;
  margin-top: 20rpx;
}
.list_sale:nth-child(1){
  margin-top: 0
}
.text_box{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 2
}
 .left_box{
  width: 207rpx;
  height: 100%;
  float: left

}
.right_box{
  width: 106rpx;
  height: 100%;
