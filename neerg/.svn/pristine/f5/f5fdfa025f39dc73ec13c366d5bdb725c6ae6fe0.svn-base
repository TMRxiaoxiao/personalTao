/* bargaining/bargaining.wxss */
.banner{
    width: 750rpx;
    height: 421rpx;
    overflow: hidden;
    position: relative;
}
.banner image{
    width: 100%;
    height: auto;
}
.btmTimeOver{
    width: 100%;
    height: 90rpx;
    background: #ff5722;
    position:absolute;
    bottom: 0;
    left: 0;
    line-height: 84rpx;
}
.btmTimeOver > image{
    width: 33rpx;
    height: 33rpx;
    margin-left: 28rpx;
    vertical-align: text-top;
}
.btmTimeOver > text:nth-child(2){
    font-size: 30rpx;
    color: #ffffff;
    margin-left: 28rpx;
}
.btmTimeOver > text:nth-child(3){
    font-size: 30rpx;
    color: #ffffff;
    margin-left: 28rpx;
    letter-spacing: 5rpx
}
.bodyNum{
    position: absolute;
    right: 30rpx;
    top: 39rpx;
    color: #ff5722;
    font-size: 24rpx;
    line-height: 25rpx;
}
.bodyNum > image{
    width:19rpx;
    height: 25rpx;
}
.kanjiaIntro{
    width: 694rpx;
    padding: 50rpx 0 66rpx 0;
    margin: 0 auto;
}
.kanjiaIntro > text:nth-child(1){
    display: block;
    font-size: 30rpx;
    color: #333333;
}
.kanjiaIntro > text:nth-child(2){
    display: block;
    font-size: 24rpx;
    color: #666666;
    line-height: 35rpx;
    margin-top: -10rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 3;
    overflow: hidden;
}
.kanjiaIntro > view{
    margin-top: 10rpx;
}
.kanjiaIntro > view > text:nth-child(1){
    color: #6a6a6a;
    font-size: 24rpx;
}
.kanjiaIntro > view > text:nth-child(2){
    color: #c72121;
    font-size: 30rpx;
    /* text-decoration: line-through */
}
progress{
    border-radius: 12rpx;
    margin-top: 60rpx;
    overflow: hidden
}
.price{
    margin-top: 14rpx !important;
    display:flex;
    justify-content: space-between;
}
.kanjiaIntro > .price > text:nth-child(1){
    font-size: 23rpx;
    color: #db3b39;
    margin-left: 10rpx;
}
.kanjiaIntro > .price > text:nth-child(2){
    font-size: 23rpx;
    color: #848484;
    margin-right: 10rpx;
}
.fenxiangBtn{
  display: block;
    width: 100%;
    height: 106rpx;
    background: #46cc8c;
    border-radius: 53rpx;
    margin-top: 70rpx !important;
    text-align: center;
    line-height: 106rpx;
    color: #ffffff;
    font-size: 36rpx;
    font-weight: bold;
}
.shiyongMall{
    width:694rpx;
    /* height: 109rpx; */
    padding-left: 28rpx;
    padding-right: 28rpx;
    background: #f8f8f8;
    padding-top: 15rpx;
    padding-bottom: 15rpx
}
.shiyongMall > image{
    width: 30rpx;
    height: auto;
    vertical-align: middle;
}
.shiyongMall >text{
    color: #333333;
    font-size: 30rpx;
    line-height: 54rpx;
    margin-left: 32rpx;
}
.huodongIntro{
    width: 694rpx;
    margin: 0 auto;
}
.huodongIntro >text:nth-child(1){
    font-size: 30rpx;
    color: #000;
    display: block;
}
.huodongIntro >text:nth-child(2){
    font-size: 24rpx;
    color: #666666;
    display: block;
    line-height: 40rpx;
}
.sellHd{
    width: 750rpx;
    height: auto;
    background: #ffffff;
    padding-top: 58rpx;
}
/*砍价英雄榜*/
.sellHero{
    width: 750rpx;
    margin-top: 50rpx;
}
.kanjiaNav{
    width: 100%;
    height: 109rpx;
    background: #f8f8f8;
    display: flex;
    justify-content: space-between;
}
.one{
    height: 97rpx;
    line-height: 97rpx;
    padding: 0 10rpx;
    color: #333333;
    font-size: 30rpx;
}
.kanjiaNav > .one:nth-child(1){
    margin-left: 134rpx;
}
.kanjiaNav > .one:nth-child(2){
    margin-right: 134rpx;
}
.active11{
    position: relative
}
.active11::after{
    content: "";
    position: absolute;
    bottom: 0rpx;
    left: 0;
    width: 100%;
    height: 4px;
    background: #46cc8c;
}
.listKj{
    width: 694rpx;
    padding: 65rpx 0 20rpx;
    margin: 0 auto;
}
.oneBox{
    width: 100%;
    height: 57rpx;
    margin-bottom: 43rpx;
}
.oneBox > image:nth-child(1){
    width: 57rpx;
    height: 57rpx;
    float: left;
    border-radius: 50%
}
.oneBox > view{
    width: 360rpx;
    float: left;
    margin-left: 20rpx;
}
.oneBox > view > text:nth-child(1){
    display: block;
    color: #333333;
    font-size: 24rpx;
}
.oneBox > view > text:nth-child(2){
    display: block;
    color: #666666;
    font-size: 22rpx;
}
.oneBox > image:nth-child(3){
    width: 44rpx;
    float: left;
    vertical-align: middle;
}
.oneBox > text:nth-child(4){
    font-size: 24rpx;
    color: #333333;
    margin-left: 30rpx;
}
.oneBox > text:nth-child(5){
    font-size: 24rpx;
    color: #3eb370;
    margin-left: 17rpx;
}
.richtext{
    width: 694rpx;
    height: auto;
    padding-top: 26rpx;
    background: #f8f8f8;
    padding-left: 28rpx;
    padding-right: 28rpx;
    overflow: hidden
}
.club{
  display:block;
  font-size:24rpx;
  color:#666666;
  line-height:35rpx;
  margin-top:-10rpx;
}