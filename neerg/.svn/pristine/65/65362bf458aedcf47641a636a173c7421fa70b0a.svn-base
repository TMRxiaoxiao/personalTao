/* personInfor/personInfor.wxss */
/* pages/my/my.wxss */
.top_mess{
  width: 750rpx;
  height: 242rpx;
  position: relative
}
.top_mess > image{
  width: 100%;
  height: 100%;
}
.main_box{
  width: 440rpx;
  height: 116rpx;
  position: absolute;
  top: 58rpx;
  left: 62rpx;
}
.body_p{
  width: 116rpx;
  height: 116rpx;
  position: absolute;
  top: 0;
  left: 0;
  border-radius: 50%;
  overflow: hidden;
  z-index: 100
}
.vip{
  width: 33rpx;
  height: 33rpx;
  position: absolute;
  left: 80rpx;
  bottom: 0;
  z-index: 100
}
.base_mess{
  position: absolute;
  left: 140rpx;
  top: 26rpx;
}
.first text:nth-child(1){
  display: block;
  float: left;
  font-size: 28rpx;
  color:#fff;
  line-height: 1;
  font-weight: bold
}
.nk_card{
  width: 94rpx;
  height: 30rpx;
  float: left;
  margin-left: 14rpx;
  position: relative
}
.nk_card image{
  display: block;
  width: 100%;
  height: 100%;
}
.text_box{
  width: 100%;
  height: 100%;
  font-size: 18rpx;
  color:#fff;
  position: absolute;
  left: 0;
  bottom: 0;
  line-height: 28rpx;
  text-align: center
}
.second{
  margin-top: 22rpx;
  font-size: 23rpx;
  color: #fff
}
.second > view{
  float: left;
  line-height: 1
}
.second > view:nth-child(2) image{
  display: block;
  width: 16rpx;
  height: 24rpx;
  margin-left: 6rpx
}
.second > view:nth-child(3){
  display: block;
  margin-left: 28rpx;
  line-height: 1
}
.second > view:nth-child(4) image{
  display: block;
  width: 22rpx;
  height: 22rpx;
  margin-left: 6rpx;
  margin-right: 4rpx
}
.vip_club{
  width: 627rpx;
  height: 115rpx;
  margin: 0 auto;
  padding-top: 40rpx;
  position: relative
  
}
.vip_club > text{
  display: block;
  font-size: 38rpx;
  color: #4b4748;
  font-weight: bold
}
.vip_club > view > text:nth-child(1){
  font-size: 21rpx;
  color: #000000
}
.vip_club > view > text:nth-child(2){
  font-size: 21rpx;
  color: #f26170
}
.kthy{
  position: absolute;
  top: 30rpx;
  right: 22rpx;
  font-size: 20rpx;
  color: #a0a0a0;
}
.right_box{
  width: 11rpx;
  height: 21rpx;
  display: inline-block;
  position: absolute;
  top: 4rpx;
  right: -20rpx
}
.right_box image{
  width:100%;
  height: 100%;
}
.kthy > text{
  line-height: 1
}
.four_nav{
  width: 627rpx;
  height: 126rpx;
  margin: 80rpx auto;
  display: flex;
  justify-content: space-between
}
.one_nav{
  width: 25%;
  height: 82%;
  position: relative
}
.imgbox{
  width: 76rpx;
  height: 58rpx;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  margin-left: auto;
  margin-right: auto
}
.imgbox image{
  width: 100%;
  height: 100%;
}
.one_nav > text{
  width: 100%;
  display: block;
  position: absolute;
  left: 0;
  bottom: 0;
  text-align: center;
  font-size: 24rpx;
  color: #292929
}
.list_message{
  width: 676rpx;
  height: 90rpx;
  border-bottom: 1px solid #dddddd;
  margin:  0 auto;
  display: flex;
  justify-content: space-between;
}
.list_message view:nth-child(1){
  margin-left: 28rpx;
  line-height: 90rpx;
  color: #333333;
  font-size: 25rpx;
}
.dir_right{
  width: 13rpx;
  height: 21rpx;
  margin-right: 36rpx;
  margin-top: 38rpx
}
.content{
  width: 694rpx;
  height: auto;
  margin: 41rpx auto;
}
.list_one{
  width: 100%;
  height: 141rpx;
  border-bottom: 1px solid #ececec;
  display: flex;
  justify-content: space-between;
  line-height: 141rpx;
}
.list_one text:nth-of-type(1){
  font-size: 32rpx;
  color: #000;
  margin-left: 18rpx;
  width: 200rpx
}
.list_one text:nth-of-type(2){
  font-size: 32rpx;
  color: #bfbfbf;
  margin-right: 34rpx
}
.pick1 text{
  font-size: 32rpx;
  color: #bfbfbf;
  margin-right: 34rpx 
}
.sub{
  width: 624rpx;
  height: 96rpx;
  background: #46cc8c;
  border-radius: 60rpx;
  text-align: center;
  line-height: 96rpx;
  color: #fff;
  font-weight: bold;
  margin: 130rpx auto;
}
