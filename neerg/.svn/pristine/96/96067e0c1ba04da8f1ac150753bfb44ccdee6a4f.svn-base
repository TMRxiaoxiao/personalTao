/* zhengdanagjiMore/zhengdanagjiMore.wxss */
/* gcourse/couse_content/couse_content.wxss */
.banner_box{
  width: 750rpx;
  height: 375rpx;
}
.banner_box image{
  width: 100%;
  height: 100%;
}
.title_desc{
  width: calc( 100vw - 54rpx);
  height: 192rpx;
  border-bottom: 2px solid #f0f0f0;
  margin:0 auto;
  padding-top: 74rpx
}
.img_desc{
  font-size: 34rpx;
  color: #333;
  font-weight: bold;
  line-height: 62rpx
}
.zan_box{
  width: 100%;
  height: 60rpx;
  margin-top: 44rpx;
}
.imgsBox{
  width: 146rpx;
  height: 100%;
  float: left;
  position: relative
}
.zan_box{
  display: flex;
  justify-content: space-between
}
.text_con{
  font-size: 34rpx;
  color: #c0c0c0;
  line-height: 60rpx;
  float: left;
  margin-left: 12rpx
}
.text_view{
  font-size: 24rpx;
  color: #c0c0c0;
  line-height: 60rpx
}
.pic_one{
  width: 60rpx;
  height: 100%;
  border-radius: 50%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 3
}
.pic_one image{
  width: 100%;
  height: 100%;
}
.pic_one:nth-child(2){
  left: 40rpx;
  z-index: 2
}
.pic_one:nth-child(3){
  left: 83rpx;
  z-index: 1
}
.content_box{
  width: calc( 100vw - 54rpx);
  margin: 0 auto;
  color: #333333;
  font-size: 22rpx;
  margin-top: 64rpx;
  line-height: 50rpx;
}
.photo_img{
  margin-top: 90rpx;
  width: 100%;
}
.photo_img image{
  width: 100%;
}
.keyWord{
  margin-top: 80rpx;
  font-size: 24rpx;
  color: #59ae7c;
}
.keyWord text{
  margin-left: 34rpx
}
.keyWord text:nth-child(1){
  margin-left: 0
}
.time{
  margin-top: 20rpx
}
.time text{
  font-size: 16rpx;
  color: #666666;
}
.zan{
  display: inline;
}
.zan image{
  width: 27rpx;
  height: 23rpx
}
.time{
  display: flex;
  justify-content: space-between
}
.zan{
  vertical-align: middle;
  margin-right: 14rpx
}
.title{
  display: flex;
  justify-content: space-between;
  padding-bottom: 34rpx;
  border-bottom: 2px solid #e9e9e9;
  margin-bottom: 50rpx
}
.title text:nth-child(1){
  font-size: 30rpx;
  color: #333333;
  font-weight: bold
}
.title text:nth-child(2){
  font-size: 24rpx;
  color: #666666;
}
.comment_list{
  width: 100%;
  height: auto;
  display: flex;
  justify-content: space-between;
  margin-bottom: 70rpx
}
.namebox{
  font-size: 18rpx;
  color: #46cc8c;
  line-height: 1
}
.photo_img_body{
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  overflow: hidden
}
.photo_img_body image{
  width: 100%;
  height: 100%;
  display: block
}
.right_name_desc{
  width: 633rpx;
}
.descbox{
  color: #333333;
  font-size: 22rpx;
  line-height: 1;
  margin-top: 20rpx
}
.before_time{
  font-size: 18rpx;
  color: #333333;
  line-height: 1;
  margin-top: 38rpx
}
.mall_res{
  padding: 28rpx;
  margin-top: 24rpx;
  background: #f8f8f8;
  font-size: 20rpx;
  color: #666666
}