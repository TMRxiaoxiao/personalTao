}
.second{
    height: 266rpx;
    background:#f8f8f8;
    margin-top: 2rpx;
    padding-top: 53rpx;
    padding-left: 30rpx;
    padding-right: 37rpx;
}
.second > image{
    width: 200rpx;
    height: 116rpx;
    border-radius: 10rpx;
    float: left;
}
.rightR{
    width: 394rpx;
    height: auto;
    float: right;
}
.rightR >text:nth-child(1){
    display: block;
    font-size: 30rpx;
    color: #333333;
}
.rightR >text:nth-child(2){
    display: block;
    font-size: 24rpx;
    color: #666;
    line-height: 37rpx;
    margin-top: 20rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}
.rightR >view{
    margin-top: 30rpx;
}
.rightR >view>text:nth-child(1){
    font-size: 24rpx;
    color: #6a6a6a;
}
.rightR >view>text:nth-child(2){
    font-size: 30rpx;
    color: #c72121;
}
