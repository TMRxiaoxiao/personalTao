    display: flex;
    justify-content: space-between;
}
.bodyPhoto{
    height: 48rpx;
    background: #fefefe;
    border-radius: 24rpx;
    line-height: 48rpx;
    color: #7d7c7d;
    font-size: 14rpx;
    padding: 7rpx 17rpx;
}
.bodyPhoto > image{
    width: 34rpx;
    height: 34rpx;
    vertical-align: middle;
    border-radius: 50%
}
.bodyPhoto > text{
    margin-left: 20rpx;
}
.num{
    line-height: 22rpx;
    margin-right: 30rpx;
    margin-top: 16rpx;
}
.num > image{
    width:19rpx;
