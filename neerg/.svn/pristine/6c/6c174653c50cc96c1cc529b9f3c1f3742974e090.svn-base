/* pages/store/store.wxss */
.search{
  width: 750rpx;
  height: 112rpx;
  border-bottom: 1px solid #e8e8e8;
  position: relative
}
.icon_box{
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  left: 90rpx;
  top: 0;
  bottom: 0;
  margin-top: auto;
  margin-bottom: auto
}
.icon_box image{
  width: 100%;
  height: 100%
}
.input{
  width: 500rpx;
  height: 52rpx; 
  position: absolute;
  left: 158rpx;
  top: 0;
  bottom: 0;
  margin-top: auto;
  margin-bottom: auto;
  padding-top: 12rpx;
  z-index: 0
}
.picker{
  width: 656rpx;
  height: 56rpx;
  margin: 46rpx auto;
  display: flex;
  justify-content: space-between
}
.everyOne{
  width: 152rpx;
  height: 56rpx;
  border: 1px solid #dfdfdf;
  border-radius: 40rpx;
  font-size: 24rpx;
  line-height: 56rpx;
  position: relative;
  padding-left: 50rpx
}
.xiala{
  width: 20rpx;
  height: 14rpx;
  position: absolute;
  right: 38rpx;
  top: 0;
  bottom: 0;
  margin-bottom: auto
}
.xiala image{
  width: 100%;
  height: 100%;
}
.adress_list{
  width: calc( 100vw - 54rpx);
  margin: 0 auto;
  border-bottom: 1px solid #e8e8e8;
  padding-top: 64rpx;
  padding-bottom: 56rpx;
  position: relative
}
.adress_list > text{
  display: block
}
.adress_list > text:nth-child(1){
  font-size: 30rpx;
  color: #000;
  display: inline-block;
  line-height: 1
}

.adress_list > text:nth-child(2){
  font-size: 22rpx;
  color: #8A8A8A;
}
.third{
  height: 35rpx;
  margin-top: 28rpx
}
.third > text{
  display: block;
  float: left;
}
.markhuodong{
  width: 34rpx;
  height: 34rpx;
  margin-left: 8rpx;
  font-size: 20rpx;
  color: #fff;
  line-height: 34rpx;
  text-align: center
}
.third > text:nth-child(1){
  margin-left: 0rpx;
  /* background: #f36541 */
}
.yuli{
  width: 88rpx;
  height: 63rpx;
  position: absolute;
  right: 22rpx;
  bottom: 60rpx;
  white-space: nowrap
}
.changqu{
  width: 88rpx;
  height: 34rpx;
  background: #42c984;
  font-size: 20rpx;
  color: #fff;
}
.changqu image{
  width: 22rpx;
  height: 22rpx;
  vertical-align: middle;
  margin-left: 12rpx
}
.changqu text{
  margin-left: 5rpx;
}
.ad{
  color: #bbbbbb;
  font-size: 20rpx;
  position: absolute;
  bottom: 0;
  left: 0
}
.you{
  width: 12rpx;
  height: 18rpx;
  position: absolute;
  top: -18rpx;
  right: 15rpx
}
.you image{
  width: 100%;
  height: 100%
}
/*地区选择*/
.infoText{
    margin-top: 20rpx;
    text-align: center;
    width: 100%;
    justify-content: center; 
}

picker-view{
  background-color: white;
  padding: 0;
  width: 100%; 
  height: 380rpx;
  bottom: 0;
  position: fixed;
}

picker-view-column view{
  vertical-align:middle; 
  font-size: 28rpx;
  line-height: 28rpx;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

/* ----------------------------------------- */

.animation-element-wrapper {
  display: flex;  
  position: fixed;
  left: 0;
  top:0;
  height: 100%;
  width: 100%;
  background-color: rgba(0, 0, 0, 0.6);
}
.animation-element {
  display: flex;
  position: fixed;
  width: 100%;
  height: 470rpx;
  bottom: 0;
  background-color: rgba(255, 255, 255, 1);
}

.animation-button {
  margin-top: 20rpx;
  top:20rpx;
  width: 400rpx;
  height: 100rpx; 
  line-height: 100rpx; 
  align-items:center;
}


.plp_text{
  color: #999999;
  display: inline-flex;  
  position: fixed;
  margin-top: 20rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  font-size: 34rpx;
  font-family: Arial, Helvetica, sans-serif;
}

.left-bt{
  left: 30rpx;
}
.right-bt {
  right: 30rpx;
}

.line{
  display: block;
  position: fixed;
  height: 1rpx;
  width: 100%;
  margin-top: 89rpx; 
  background-color: #eeeeee;
}
.huodong{
  color:#808080;font-size:24rpx !important;
  margin-left: 10rpx;
  width: 500rpx
}
.zanwushuju{
  width: 400rpx;
  height: 580rpx;
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0rpx;
  right: 0;
  margin: auto;
  z-index: -1;
  opacity: 0.7
}
.zanwushuju image{
  width: 100%;
  height: 100%
}