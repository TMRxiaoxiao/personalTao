/* freeTest/freeTest.wxss */
.test_ban{
  width: 750rpx;
  height: 264rpx;
  background: black;
  position: relative
}
.test_ban image{
  width: 100%;
  height: 100%
}
.cover{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: rgba(0,0,0,0.7)
}
.form{
  width: 562rpx;
  height: auto;
  margin: 61rpx auto;
  white-space: nowrap
}
.test_box{
  font-size: 31rpx;
  color: #000;
  font-weight: bold;
  text-align: center;
}
.name{
  width: 100%;
  height: 68rpx;
  font-size: 25rpx;
  color: #fff;
  background: #626262;
  margin-top: 50rpx;
}
.name > text{
  display: block;
  float: left;
  margin-left: 38rpx;
  line-height: 68rpx

}
.name1{
  width: 100%;
  font-size: 25rpx;
  color: #fff;
  background: #626262;
  margin-top: 50rpx;
  height: 138rpx
}
.name1 > text{
  display: block;
  float: left;
  margin-left: 38rpx;
  line-height: 68rpx
}
.input1{
  width: 450rpx;
  float: right;
margin-top: 15rpx
}
.input2{
  width: 450rpx;
  height: 118rpx;
  float: right;
margin-top: 6rpx
}
.submit_button{
  width: 100%;
  height: 96rpx;
  background: #46cc8c;
  border-radius: 50rpx;
  margin-top: 84rpx;
  font-size: 33rpx;
  color: #fff;
  line-height: 88rpx;
  text-align: center;
  font-weight: bold
}
.cover image{
  width: 42rpx;
  height: 62rpx;
  margin-top: 72rpx;
  margin-left: 62rpx;
  vertical-align: bottom;
}
.cover text{
  color: #46cc8c;
  font-size: 48rpx;
  margin-left: 8rpx
}