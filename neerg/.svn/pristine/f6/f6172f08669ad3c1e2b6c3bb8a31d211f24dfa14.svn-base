/* sellGroup/sellGroup.wxss */
.banner{
    width: 750rpx;
    /*height: 450rpx;*/
    overflow: hidden;
}
.banner > image{
    width: 100%;
    display: block;
}
.main1{
    width: 750rpx;
    height: auto;
    background: #f8f8f8;
}
.intro{
    width: 100%;
    height: auto;
    padding: 60rpx 0 36rpx 0;
    background: #ffffff;
}
.main{
    width: 694rpx;
    height: auto;
    margin: 0 auto;
}
.main > text:nth-child(1){
    font-size: 30rpx;
    color: #333333;
    display: block;
}
.main > text:nth-child(2){
    font-size: 24rpx;
    color: #666666;
    display: block;
    line-height: 35rpx;
    margin-top: -10rpx;
    display: -webkit-box;
-webkit-box-orient: vertical;
-webkit-line-clamp: 2;
overflow: hidden;
}
.priceBox{
    margin-top: 38rpx;
    overflow: hidden
}
.priceBox > view{
  width:500rpx;
  float: left
}
.priceBox > view > text:nth-child(1){
    font-size: 24rpx;
    color: #c72121;
}
.priceBox > view > text:nth-child(2){
    font-size: 24rpx;
    color: #c72121;
}
.priceBox > view> text:nth-child(3){
    font-size: 30rpx;
    color: #c72121;
}
.priceBox > view> text:nth-child(4){
    font-size: 24rpx;
    color: #c4c4c4;
    margin-left: 30rpx;
}
.priceBox > view> text:nth-child(5){
    font-size: 24rpx;
    color: #c4c4c4;
    text-decoration:line-through;
}
.priceBox > text:nth-child(2){
  float: right;
    font-size: 24rpx;
    color: #fbbb2a;
    border: 1px solid #fcc739;
    padding: 5rpx 20rpx 5rpx 20rpx;
    border-radius: 24rpx;
    display: block;
    margin-top: 10rpx
}
.groupList{
    width: 750rpx;
    padding: 68rpx 0rpx 72rpx 0rpx;
    background: #ffffff;
    margin-top: 26rpx;
}
.tab-h{
    width: 100%;
    height: 120rpx;
    overflow: hidden;
    white-space: nowrap;
}

.tab-item{
    width: 222rpx;
    height: 120rpx;
    background: #f8f8f8;
    box-shadow: 0 0 10rpx #f3f3f3;
    border-radius: 20rpx;
    margin-left: 45rpx;
    display: inline-block;
    color: #333;
    position: relative;
}
.tab-h > .tab-item:nth-child(1){
    margin-left: 0 !important;
}
.tab-item > image{
    width: 120rpx;
    position: absolute;
    top: 0rpx;
    left: -15rpx;
}
.tab-item > text:nth-child(2){
    font-size: 24rpx;
    color: #ffffff;
    position: absolute;
    top: 4rpx;
    left: 20rpx;
}
.price{
    display: block;
    width: 100%;
    color: #333333;
    position: absolute;
    top: 50rpx;
    left: 0;
    text-align: center;
}
.price > text:nth-child(1){
     color: #333333;
     font-size: 30rpx;
 }
.price > text:nth-child(2){
    color: #333333;
    font-size: 36rpx;
}
.shiyongMall{
    width:694rpx;
    background: #f8f8f8;
    padding-top: 12rpx;
    padding-bottom: 12rpx;
    padding-left: 28rpx;
    padding-right: 28rpx
}
.shiyongMall > image{
    width: 30rpx;
    height: auto;
    vertical-align: middle;
}
.shiyongMall >text{
    color: #333333;
    font-size: 30rpx;
    line-height: 58rpx;
    margin-left: 20rpx;
}
.huodongIntro{
    width: 694rpx;
    margin: 0 auto;
}
.huodongIntro >text:nth-child(1){
    font-size: 24rpx;
    color: #333333;
    display: block;
}
.huodongIntro >text:nth-child(2){
    font-size: 24rpx;
    color: #666666;
    display: block;
    line-height: 40rpx;
}
.sellHd{
    width: 750rpx;
    height: auto;
    background: #ffffff;
    padding-top: 58rpx;
    padding-bottom: 62rpx;
}
.sellList{
    width: 750rpx;
    height: auto;
    background: #ffffff;
    margin-top: 26rpx;
    padding:58rpx 0 60rpx 0;
}
.first{
    width: 694rpx;
    margin: 0 auto;
    display: flex;
    justify-content: space-between;
    color: #333;
    font-size: 24rpx;
}
.second{
    width: 694rpx;
    margin: 46rpx auto 0;
}
.group{
    width: 100%;
    height: 61rpx;
    margin-top: 40rpx;
}
.second > .group:nth-child(1){
    margin-top: 0;
}
.group >view{
    float: left;
}
.group >view:nth-child(1){
    font-size: 24rpx;
    width: 414rpx;
}
.group >view:nth-child(1) > image{
    width: 57rpx;
    height: 57rpx;
    vertical-align: middle;
    border-radius: 50%
}
.group >view:nth-child(2){
    font-size: 24rpx;
    width: 160rpx
}
.group >view:nth-child(2)> view:nth-child(1)>text:nth-child(2){
    color: #3eb370;
}
.group >view:nth-child(2)> view:nth-child(2)>text{
    color: #666;
}
.group >view:nth-child(3){
    font-size: 24rpx;
    line-height: 60rpx;
    margin-left: 10rpx;
    width: 110rpx;
    height: 60rpx;
    border: 1px solid #666666;
    text-align: center;
    border-radius: 10rpx;
    margin-top: 5rpx;
    color: #666666;
}
.group >view:nth-child(4){
    font-size: 24rpx;
    color: #666666;
}
.group > view:nth-child(1)>text:nth-child(2){
    margin-left: 22rpx;
}
.group > view:nth-child(1)>text:nth-child(3){
    color: #46cc8c;
    margin-left: 10rpx;
    margin-right: 10rpx;
}
.richText{
    width: 694rpx;
    background: #f8f8f8;
    padding-top: 26rpx;
    padding-bottom: 26rpx;
    margin-bottom: 64rpx;
    padding-left: 28rpx;
    padding-right: 28rpx;
    overflow: hidden;
}
.richText  image{
    width: 750rpx;
    max-width: 750rpx
}
.buyBtn{
    width: 750rpx;
    height: 100rpx;
    background: pink;
    position: fixed;
    left: 0;
    bottom: 0;
    display: flex;
    justify-content: space-between;
}
.buyBtn>view:nth-child(1){
    width: 50%;
    height: 100%;
    background: #79daac;
    color: #fff;
    font-size:  36rpx;
    text-align: center;
    line-height: 100rpx;
}
.buyBtn>view:nth-child(2){
    width: 50%;
    height: 100%;
    background: #46cc8c;
    color: #fff;
    font-size:  36rpx;
    text-align: center;
    line-height: 100rpx;
}
.bac{
  background: #d7d7d7;
}